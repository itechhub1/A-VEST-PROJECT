e));\n  }, []);\n\n  const renturnInvestment = (investment) => {\n    if (investment.length === 0 && !loading) {\n      return (\n        <div className=\"w-full\">\n          <p className=\"text-center\">You have No investment Yet.</p>\n        </div>\n      );\n    } else {\n      <div className=\"w-full\">\n        <p className=\"text-center\">Loading....</p>\n      </div>;\n    }\n\n    return investment.map((inv) => (\n      <>\n        <TableRow key={inv._id}>\n          <TableCell component=\"th\" scope=\"row\">\n            {inv._id}\n          </TableCell>\n          <TableCell align=\"center\">\n            {inv.plan}({inv.percentage})\n          </TableCell>\n          <TableCell align=\"center\">\n            {inv.amount.toString().replace(/(.)(?=(\\d{3})+$)/g, \"$1,\")}\n          </TableCell>\n          <TableCell align=\"center\">\n            {inv.roi.toString().replace(/(.)(?=(\\d{3})+$)/g, \"$1,\")}\n          </TableCell>\n          <TableCell align=\"center\">\n            {inv.status === \"1\" ? (\n              <span className=\"text-yellow-800 p-2 bg-yellow-100 rounded-sm text-xs\">\n                Pending\n              </span>\n            ) : inv.status === \"2\" ? (\n              <span className=\"text-red-800 p-2 bg-red-100 rounded-sm text-xs\">\n                Canceled\n              </span>\n            ) : inv.status === \"3\" ? (\n              <span className=\"text-red-800 p-2 bg-red-100 rounded-sm text-xs\">\n                Expired\n              </span>\n            ) : (\n              <span className=\"text-green-800 p-2 bg-green-100 rounded-sm text-xs\">\n                Running\n              </span>\n            )}\n          </TableCell>\n          <TableCell align=\"center\">\n            {inv.payment ? (\n              <span className=\"text-green-800 p-2 px-8 text-center bg-green-100 rounded-sm text-xs \">\n                Paid\n              </span>\n            ) : (\n              <span className=\"text-red-800 p-2 px-8 text-center w-32  bg-red-100 rounded-sm text-xs  \">\n                Not paid\n              </span>\n            )}\n          </TableCell>\n          {inv.status === \"1\" ? (\n            <TableCell align=\"center\">\n              {inv.paymentPlan === \"paystack\" ? (\n                <MakePayment amount={inv.amount} investmentId={inv._id} />\n              ) : (\n                <p className=\"italic text-gray-800 text-xs\">\n                  selected bank deposit\n                </p>\n              )}\n            </TableCell>\n          ) : (\n            <TableCell align=\"center\">Payment succeded</TableCell>\n          )}\n          <TableCell align=\"center\">\n            <ButtonGroup>\n              <Link to={`/dashboard/details/${inv._id}`}>\n                <Button\n                  color=\"primary\"\n                  variant=\"contained\"\n                  size=\"small \"\n                  style={{ marginRight: \"4px\", fontSize: \"8px\" }}\n                >\n                  Details\n                </Button>\n              </Link>\n              {inv.status === \"0\" && (\n                <Button\n                  onClick={() => setopen({ id: inv._id, status: true })}\n                  color=\"secondary\"\n                  variant=\"contained\"\n                  size=\"small\"\n                  style={{ fontSize: \"8px\" }}\n                >\n                  cancel\n                </Button>\n              )}\n            </ButtonGroup>\n          </TableCell>\n        </TableRow>\n      </>\n    ));\n  };\n\n  const table = useStyles();\n  const tablecell = tableCell();\n  return (\n    <div className=\"w-full\">\n      <h1 className=\"font-semibold text-3xl mb-4\">Investments</h1>\n      <TableContainer component={Paper}>\n        <Table className={table.table} aria-label=\"simple table\">\n          <TableHead className={table.root}>\n            <TableRow>\n              <TableCell className={tablecell.root}>ID</TableCell>\n              <TableCell className={tablecell.root}>Plan</TableCell>\n              <TableCell className={tablecell.root} align=\"center\">\n                Initial Capital\n              </TableCell>\n              <TableCell className={tablecell.root} align=\"center\">\n                ROI\n              </TableCell>\n              <TableCell className={tablecell.root} align=\"center\">\n                Status\n              </TableCell>\n              <TableCell className={tablecell.root} align=\"center\">\n                Payment Status\n              </TableCell>\n              <TableCell className={tablecell.root} align=\"center\">\n                Payment Type\n              </TableCell>\n              <TableCell className={tablecell.root} align=\"center\">\n                Actions\n              </TableCell>\n            </TableRow>\n          </TableHead>\n          <TableBody> {renturnInvestment(investment)} </TableBody>\n        </Table>\n      </TableContainer>\n      {open !== null && (\n        <Modal>\n          <ModalMssg\n            action={() => {\n              deleteInvestement(open.id);\n              setopen(null);\n            }}\n            cancel={() => setopen(null)}\n          />\n        </Modal>\n      )}\n    </div>\n  );\n};\nconst mapStateToProps = (state) => {\n\n  return { investment: state.investments };\n};\n\nexport default connect(mapStateToProps, { deleteInvestement, allInvestment })(\n  Invest\n);\n","/home/tohshine/Desktop/repository/A-VEST-PROJECT/client/src/layout/dashboard/investors/plan/index.js",[],"/home/tohshine/Desktop/repository/A-VEST-PROJECT/client/src/action/investments/view.js",[],"/home/tohshine/Desktop/repository/A-VEST-PROJECT/client/src/components/investments/otherDetails.jsx",[],"/home/tohshine/Desktop/repository/A-VEST-PROJECT/client/src/reducers/investments/view.js",[],"/home/tohshine/Desktop/repository/A-VEST-PROJECT/client/src/action/index.js",[],"/home/tohshine/Desktop/repository/A-VEST-PROJECT/cli